{
  "name": "playerio.js",
  "version": "0.1.2",
  "description": "An open-source JavaScript library for leveraging client-side functionality of Player.IO.",
  "main": "lib/index.js",
  "directories": {
    "lib": "./lib",
    "example": "./example"
  },
  "dependencies": {
    "babel": "^5.8.29",
    "babel-core": "^6.0.14",
    "babel-preset-es2015": "^6.0.14",
    "bytebuffer": "^5.0.0",
    "net-browserify": "^0.2.3",
    "protobufjs": "^5.0.1",
    "protocol-buffers": "^3.1.3",
    "ws": "^0.8.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babelify": "^6.4.0",
    "brfs-babel": "^1.0.0",
    "browserify": "^12.0.1",
    "esdoc": "^0.4.3",
    "eslint": "^1.8.0",
    "exorcist": "^0.4.0",
    "long": "^3.2.0",
    "rimraf": "^2.4.3",
    "uglify-js": "^2.5.0"
  },
  "engines": {
    "node": "~4.0.0"
  },
  "scripts": {
    "start": "cd example && node server.js",
    "build-docs": "rimraf esdoc && mkdir esdoc && esdoc -c ./esdoc.json",
    "bundle": "rimraf bin && mkdir bin && cd bin && node ../browser_build.js | exorcist playerio.js.map > playerio.js && uglifyjs playerio.js -o playerio.min.js --in-source-map playerio.js.map --source-map playerio.js.min.map"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yonom/playerio.js.git"
  },
  "bugs": {
    "url": "https://github.com/Yonom/playerio.js/issues"
  },
  "browser": {
    "net": "net-browserify"
  },
  "license": "MIT"
}
